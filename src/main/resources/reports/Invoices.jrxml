	<parameter name="s_id" class="java.lang.Long"/>
	<query language="SQL"><![CDATA[SELECT 
	cc.*,
    s.selling_id,
    s.amount_total,
    s.total,
    s.total_bs,
    s.method,
    u.firstname,
    u.lastname,
    sp.selling_product_id,
    sp.amount,
    sp.purchase,
    sp.subTotal,
    sp.purchase_bs,
    sp.subTotal_bs,
    p.product_id,
    p.description,
    cl.firstname,
    cl.lastname,
    cl.type,
    cl.cedula,
    cl.code,
    cl.phone,
    cl.address
FROM 
    commerce cc, Sellings s
 JOIN
 	clients cl ON s.client_id = cl.client_id
JOIN 
    selling_products sp ON s.selling_id = sp.selling_id 
JOIN 
    users u ON u.user_id = s.registered_by 
JOIN 
    products p ON p.product_id = sp.product_id
WHERE 
    s.selling_id = $P{s_id}]]></query>
